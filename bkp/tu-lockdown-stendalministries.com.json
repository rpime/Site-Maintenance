{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowBasicS3Operations",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::boldgrid-backup-stendalministries.com",
        "arn:aws:s3:::boldgrid-backup-stendalministries.com/*"
      ]
    }
  ]
}
